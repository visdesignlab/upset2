{
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/dist/**"
      ]
    },
    "userdocs#build": {
      "dependsOn": [
        "^builddocs"
      ],
      "outputs": [
        "**/build/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": false
    },
    "storybook": {
      "cache": false
    }
  }
}
